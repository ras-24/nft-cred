# NFTCred contract address
NEXT_PUBLIC_NFTCRED_CONTRACT=nftcred_contract_address_here

# Block explorer
NEXT_PUBLIC_BLOCK_EXPLORER_URL=block_explorer_url_here

# Blockchain node endpoint
RPC_URL=your_rpc_url_here

# USDC contract address which will be used as loan funds
USDC_CONTRACT=usdc_contract_address_here

# Wallet Private Key for Next.js Backend and Smart Contract interaction only
PRIVATE_KEY=your_private_key_here

# Neon database connection generated by Vercel
# Recommended for most uses
DATABASE_URL=your_database_url_here

# For uses requiring a connection without pgbouncer
DATABASE_URL_UNPOOLED=your_database_url_unpooled_here

# Parameters for constructing your own connection string
PGHOST=your_pghost_here
PGHOST_UNPOOLED=your_pghost_unpooled_here
PGUSER=your_pguser_here
PGDATABASE=your_pgdatabase_here
PGPASSWORD=your_pgpassword_here

# Parameters for Vercel Postgres Templates
POSTGRES_URL=your_postgres_url_here
POSTGRES_URL_NON_POOLING=your_postgres_url_non_pooling_here
POSTGRES_USER=your_postgres_user_here
POSTGRES_HOST=your_postgres_host_here
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DATABASE=your_postgres_database_here
POSTGRES_URL_NO_SSL=your_postgres_url_no_ssl_here
POSTGRES_PRISMA_URL=your_postgres_prisma_url_here